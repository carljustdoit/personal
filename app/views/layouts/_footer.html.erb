// Mouse Click Events
$(document).click(function(event){
	var target = $(event.target);
	if (target.hasClass('carousel-control')) {
		if (target.hasClass('right')) {
			window.tracker('trackStructEvent','click','crousel-navigation','right','');
			mixpanel.track("Right Carousel Click");
		}
		if (target.hasClass('left')) {
			window.tracker('trackStructEvent','click','crousel-navigation','left','');
			mixpanel.track("Left Carousel Click");
		}
	}
});

// Keyboard Events
$(document).keypress(function(event){
	var target = $(event.target);
	if (target.hasClass('input'))
		window.tracker('trackStructEvent','input','text-input', target.val(),'');
});



