<%= render 'layouts/header' %>


<!-- Carousel
================================================== -->
<div id="myCarousel" class="carousel slide">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
    <li data-target="#myCarousel" data-slide-to="3"></li>
  </ol>
  <div class="carousel-inner">
    <div class="item active">
      <img src="https://s3.amazonaws.com/carl-liu/pictures/huixiang_resized.jpg">
      <div class="container">
        <div class="carousel-caption">
          <p class="cap-text">Funnel, Sunset at Redwood Shores</p>
        </div>
      </div>
    </div>
    <div class="item">
      <img src="https://s3.amazonaws.com/carl-liu/pictures/GoldenGate_resized.jpg">
      <div class="container">
        <div class="carousel-caption">
          <p class="cap-text">Golden Gate Bridge</p>
        </div>
      </div>
    </div>
    <div class="item">
      <img src="https://s3.amazonaws.com/carl-liu/pictures/Beijing_flickr.jpg">
      <div class="container">
        <div class="carousel-caption">
          <p class="cap-text">Beijing Landscape</p>
        </div>
      </div>
    </div>
    <div class="item">
      <img src="https://s3.amazonaws.com/carl-liu/pictures/beebelake2.jpg">
      <div class="container">
        <div class="carousel-caption">
          <p class="cap-text">Fall at Beebe Lake, Cornell</p>
        </div>
      </div>
    </div>
  </div>
  <!-- Controls -->
  <a class="left carousel-control" href="#myCarousel" data-slide="prev">
    <i class="glyphicon glyphicon-chevron-left"></i>
  </a>
  <a class="right carousel-control" href="#myCarousel" data-slide="next">
    <i class="glyphicon glyphicon-chevron-right"></i>
  </a>  
</div>
<!-- /.carousel -->

<%= render 'layouts/footer' %>

